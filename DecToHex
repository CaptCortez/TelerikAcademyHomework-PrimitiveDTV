using System;

namespace Hexadecimal
{
    class DecToHex
    {
        static void Main()
        {
            Console.Title = "Convertion from Decimal to Hexadecimal numeral system";
            int hex = 0xFE;

            Console.WriteLine("The value of the hex number 0x100 in dec is: " + hex);
        }
    }
}
